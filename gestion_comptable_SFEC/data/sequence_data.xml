<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Séquence pour les partenariats exclusifs -->
    <record id="seq_partenariat_exclusif" model="ir.sequence">
        <field name="name">Partenariat Exclusif</field>
        <field name="code">gestion_comptable_sfec.partenariat_exclusif</field>
        <field name="prefix">PRT%</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Séquence pour les accusés de réception -->
    <record id="seq_accuse_reception" model="ir.sequence">
        <field name="name">Accusé Réception</field>
        <field name="code">gestion_comptable_sfec.accuse_reception</field>
        <field name="prefix">ACR%</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Séquence pour les notifications -->
    <record id="seq_notification" model="ir.sequence">
        <field name="name">Notification</field>
        <field name="code">gestion_comptable_sfec.notification</field>
        <field name="prefix">NOT%</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Séquence pour les relances -->
    <record id="seq_relance" model="ir.sequence">
        <field name="name">Relance</field>
        <field name="code">gestion_comptable_sfec.relance</field>
        <field name="prefix">RLC%</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
